  "description": "Frontend application for crowdsourcing engine - Funcrowd",
  "scripts": {
    "build:prod": "webpack --mode production",
    "analyse": "webpack --profile --json",
    "start": "node src/server/index.js",
    "start:prod": "npm run build:prod && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
